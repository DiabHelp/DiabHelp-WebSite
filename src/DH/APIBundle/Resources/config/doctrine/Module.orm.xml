<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="DH\APIBundle\Entity\Module" table="module">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="name" type="string" column="name" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="description" type="text" column="description" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="type" type="string" column="type" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="organisme" type="string" column="organisme" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="logo" type="text" column="logo" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="note" type="float" column="note" precision="10" scale="0" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="nbVote" type="integer" column="nb_vote" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="display" type="integer" column="display" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">1</option>
      </options>
    </field>
    <field name="version" type="string" column="version" length="42" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="default">0.0.1</option>
      </options>
    </field>
    <field name="isnew" type="integer" column="isNew" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">1</option>
      </options>
    </field>
    <field name="urlstore" type="text" column="urlStore" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="urlsiteweb" type="text" column="urlSiteWeb" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="size" type="text" column="size" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
